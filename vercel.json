{
  "version": 2,
  "builds": [
    {
      "src": "backend/server.ts",
      "use": "@vercel/node"
    },
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/routes/api/v1/(.*)",
      "dest": "backend/routes/api/v1/$1"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/$1"
    }
  ]
}