{
  "version": 2,
  "builds": [
    {
      "src": "backend/package.json",
      "use": "@vercel/node"
    },
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/routes/v1/(.*)",
      "dest": "src/backend/routes/v1/$1"
    },
    {
      "src": "/(.*)",
      "dest": "src/frontend/$1"
    }
  ]
}